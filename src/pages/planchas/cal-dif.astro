---
import "../../styles/global.css";
import ContainerCardExam from "../../components/ContainerCardExam.astro";
import CardExam from "../../components/CardExam.astro"
---

<html lang="es">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  

  <title>Cálculo Diferencial - TrikaWeb</title> 
  
  <link rel="icon" href="/img/icono.png" />
</head>
<body class="bg-[#101010] text-neutral-100 antialiased">
  

  <div class="flex min-h-screen w-full flex-col items-center gap-12">

  <header class="w-full bg-[#171A1F]">
  
  <div class="mx-auto max-w-[1400px] px-8">

    <div class="flex items-center justify-between py-6">
      <div class="flex w-[188px] items-center justify-between">
        <img src="/img/logo2.png" alt="TrikaWeb" class="h-auto w-auto" />
      </div>
      <nav class="flex items-center gap-10">
        <a class="text-[20px] leading-7 font-medium hover:text-[#51A624]" href="/planchas">Planchas</a>
        <a class="text-[20px] leading-7 font-medium hover:text-[#51A624]" href="/profesores">Profesores</a>
        <button class="rounded-xl p-2 hover:bg-neutral-800/60" aria-label="Inicio">
          <span class="block h-[19px] w-[18px] border-2 border-[#E6E9EF]"></span>
        </button>
      </nav>
    </div>
    
    <nav class="mt-1 mb-3 flex w-full items-center gap-2 overflow-x-auto pb-2 md:gap-4">
      <div class="mr-auto">
        <h1 class="text-3xl font-medium text-[#51A624] md:text-4xl">Cálculo Diferencial</h1>
      </div>

      <a href="#pc1" class="whitespace-nowrap rounded-lg px-3 py-1 text-gray-300 hover:bg-[#171A1F] hover:text-[#51A624]">PC1</a>
      <a href="#pc2" class="whitespace-nowrap rounded-lg px-3 py-1 text-gray-300 hover:bg-[#171A1F] hover:text-[#51A624]">PC2</a>
      <a href="#pc3" class="whitespace-nowrap rounded-lg px-3 py-1 text-gray-300 hover:bg-[#171A1F] hover:text-[#51A624]">PC3</a>
      <a href="#pc4" class="whitespace-nowrap rounded-lg px-3 py-1 text-gray-300 hover:bg-[#171A1F] hover:text-[#51A624]">PC4</a>
      <a href="#ep" class="whitespace-nowrap rounded-lg px-3 py-1 text-gray-300 hover:bg-[#171A1F] hover:text-[#51A624]">EP</a>
      <a href="#ef" class="whitespace-nowrap rounded-lg px-3 py-1 text-gray-300 hover:bg-[#171A1F] hover:text-[#51A624]">EF</a>
    </nav>
    
  </div> </header>


<main class="mx-auto flex w-full max-w-[1300px] flex-col items-center gap-12 px-4 py-2 md:px-10">
  

<!-- PC1 -->

  <section id="pc1" class="w-full scroll-mt-24 rounded-2xl bg-[#171A1F] px-6 md:px-16 py-10 md:py-12">
      <ContainerCardExam titulo="Práctica Calificada 1" />
      <div class="flex flex-wrap justify-center gap-12 py-4 my-8">
        <CardExam ciclo="25-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="22-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
      </div>
    </section>


  <!--PC 2 -->
  <section id="pc2" class="w-full scroll-mt-24 rounded-2xl bg-[#171A1F] px-6 md:px-16 py-10 md:py-12">
      <ContainerCardExam titulo="Práctica Calificada 2" />
      <div class="flex flex-wrap justify-center gap-12 py-4 my-8">
        <CardExam ciclo="25-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="22-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
      </div>
    </section>


<!--PC 3 -->

<section id="pc3" class="w-full scroll-mt-24 rounded-2xl bg-[#171A1F] px-6 md:px-16 py-10 md:py-12">
      <ContainerCardExam titulo="Práctica Calificada 3" />
      <div class="flex flex-wrap justify-center gap-12 py-4 my-8">
        <CardExam ciclo="25-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="22-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
      </div>
    </section>

<!--PC 4 -->
  <section id="pc4" class="w-full scroll-mt-24 rounded-2xl bg-[#171A1F] px-6 md:px-16 py-10 md:py-12">
      <ContainerCardExam titulo="Práctica Calificada 4" />
      <div class="flex flex-wrap justify-center gap-12 py-4 my-8">
        <CardExam ciclo="25-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="22-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
      </div>
  </section>
  

<!-- EXAMEN PARCIAL -->

  <section id="ep" class="w-full scroll-mt-24 rounded-2xl bg-[#171A1F] px-6 md:px-16 py-10 md:py-12">
      <ContainerCardExam titulo="Examen Parcial" />
      <div class="flex flex-wrap justify-center gap-12 py-4 my-8">
        <CardExam ciclo="25-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="22-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
      </div>
  </section>
  

<!--EXAMEN FINAL -->

  <section id="ef" class="w-full scroll-mt-24 rounded-2xl bg-[#171A1F] px-6 md:px-16 py-10 md:py-12">
      <ContainerCardExam titulo="Examen Final" />
      <div class="flex flex-wrap justify-center gap-12 py-4 my-8">
        <CardExam ciclo="25-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="24-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={false} />
        <CardExam ciclo="23-I" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
        <CardExam ciclo="22-II" profesor="Prof. Arámbulo Ostos" tieneSolucion={true} />
      </div>
  </section>

</main>

    <!-- 6. Footer -->
    <footer class="w-full bg-black py-3 text-center text-[16px] leading-6">
      © {new Date().getFullYear()} TrikaWeb. Todos los derechos reservados.
    </footer>
    
  </div>
</body>
</html>
